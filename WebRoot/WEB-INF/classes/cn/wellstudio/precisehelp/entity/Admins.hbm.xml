<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.wellstudio.precisehelp.entity.Admins" table="admins" catalog="precisehelp">
        <id name="admId" type="java.lang.String">
            <column name="adm_id" length="60" />
            <generator class="uuid.hex" />
        </id>
        <property name="admAccount" type="java.lang.String">
            <column name="adm_account" length="20" />
        </property>
        <property name="admPower" type="java.lang.Integer">
            <column name="adm_power" />
        </property>
        <property name="admPsw" type="java.lang.String">
            <column name="adm_psw" length="50" />
        </property>
    </class>
</hibernate-mapping>
